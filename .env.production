# Production environment variables for Vercel
# These will be used during build time

# Environment
VITE_ENVIRONMENT=production
VITE_DEBUG=false

# Feature Flags
VITE_ENABLE_MOCK_DATA=true
VITE_USE_AWS_API=false
VITE_USE_LOCAL_DYNAMODB=true
VITE_USE_LOCAL_LAMBDA=true

# WebSocket (disabled for demo)
VITE_ENABLE_WEBSOCKET=false
VITE_DEBUG_WEBSOCKET=false

# Performance Settings
VITE_VIRTUAL_SCROLL_ENABLED=true
VITE_CACHE_TIMEOUT=30000
VITE_BATCH_SIZE=100

# Demo Mode
VITE_DEMO_MODE=true
VITE_ENABLE_DEVTOOLS=false
EOF < /dev/null